<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.controller.OrderListController">
    
    <TableView fx:id="orderTable">
        <columns>
            <TableColumn fx:id="orderIdColumn" text="Order ID" />
            <TableColumn fx:id="orderDateColumn" text="Order Date" />
            <TableColumn fx:id="orderTotalColumn" text="Total Price" />
        </columns>
    </TableView>

</VBox>
